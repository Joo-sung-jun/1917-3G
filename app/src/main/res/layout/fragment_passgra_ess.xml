<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PassgraEss">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="40dp"
        android:layout_marginRight="30dp"
        android:background="#E0E9EF"
        android:orientation="vertical">

        <TextView
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="@color/white"
            android:forceDarkAllowed="false"
            android:gravity="center"
            android:text="영어인증제 (택 1)"
            android:textSize="18sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="30dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:src="@drawable/ic_baseline_auto_stories_24"
                android:layout_marginLeft="30dp"/>

            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginLeft="20dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:text="교과목 이수"
                    android:textStyle="bold"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:text="- 대학영어1, 대학영어2 이수"></TextView>
            </LinearLayout>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"></CheckBox>



        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:src="@drawable/ic_baseline_desktop_mac_24"
                android:layout_marginLeft="30dp"/>

            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginLeft="20dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:text="EZ프로그램 이수"
                    android:textStyle="bold"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:text="- English-Zone 영어 이수"></TextView>
            </LinearLayout>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"></CheckBox>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="40dp"
                android:src="@drawable/ic_baseline_badge_24"
                android:layout_marginLeft="30dp"/>

            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginLeft="20dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:text="공인 어학 인증 시험 (택 1)"
                    android:textStyle="bold"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:text="- TEPS 710점 이상"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:text="- TOEIC 800점 이상"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:text="- TOEFL(IBT 기준) 86점 이상"></TextView>
            </LinearLayout>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="40dp"></CheckBox>

        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:layout_marginTop="370dp"
        android:orientation="vertical"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        android:background="#E0E9EF">

        <TextView
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="@color/white"
            android:forceDarkAllowed="false"
            android:gravity="center"
            android:text="꿈미래개척"
            android:textSize="18sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:src="@drawable/ic_baseline_auto_graph_24"
            android:layout_marginTop="30dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="20dp">

            <LinearLayout
                android:layout_width="60dp"
                android:layout_height="100dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="30dp"
                android:orientation="vertical">

                <CheckBox
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"></CheckBox>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:gravity="center"
                    android:text="1회차"></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="60dp"
                android:layout_height="100dp"
                android:layout_marginRight="10dp"
                android:orientation="vertical">

                <CheckBox
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"></CheckBox>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:gravity="center"
                    android:text="2회차"></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="60dp"
                android:layout_height="100dp"
                android:layout_marginRight="10dp"
                android:orientation="vertical">

                <CheckBox
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"></CheckBox>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:gravity="center"
                    android:text="3회차"></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="60dp"
                android:layout_height="100dp"
                android:orientation="vertical">

                <CheckBox
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"></CheckBox>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:forceDarkAllowed="false"
                    android:gravity="center"
                    android:text="4회차"></TextView>

            </LinearLayout>
        </LinearLayout>




    </LinearLayout>

</FrameLayout>