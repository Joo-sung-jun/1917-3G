            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:text="교과목 이수(GNU인성)" />
        <TextView
            android:id="@+id/tv_pass1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
